  <div class="panel panel-default">
    <div class="panel-heading" role="tab" id="<%== @id %>">
      <h4 class="panel-title">
        <a <%== 'class="collapsed" ' if !@expand_stage %>role="button" data-toggle="collapse" data-parent="#tournamentRoleAccordion" href="#<%== @body_id %>" aria-expanded="<%== if @expand_stage then 'true' else 'false' end %>" aria-controls="<%== @body_id %>">
          <%== @tournament_role_group %>
        </a>
      </h4>
    </div>
    <div id="<%== @body_id %>" class="panel-collapse collapse<%== ' in' if @expand_stage %>" role="tabpanel" aria-labelledby="<%== @id %>">
      <div class="panel-body">
        <% @roles.each do |r| %>
          <% @role = r[:role] %>
          <% @id = r[:id] %>
          <% @role_camel = camelcase(@role, :upper) %>
          <% @teams = r[:teams] %>
          <% @selected = r[:selected] %>
          <%== erb :'admin/tournament_role' %>
        <% end %>
      </div>
    </div>
  </div>
