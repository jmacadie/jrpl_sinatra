<% if locals[:shade] %>
<tr style="background-color: #f4ecdc;">
<% else %>
<tr>
<% end %>
  <td style="font-family: Helvetica, arial, sans-serif;
             font-size: 14px;
             color: #666666;
             text-align:left;
             line-height: 16px;
             white-space: nowrap;
             vertical-align: top;
             border-collapse: collapse;
             border-bottom: 1px solid #b7a075;"
        align="left">
    <%== locals[:prediction][:user] %>
  </td>
  <td style="font-family: Helvetica, arial, sans-serif;
             font-size: 14px;
             color: #666666;
             text-align:left;
             line-height: 16px;
             white-space: nowrap;
             border-collapse: collapse;
             border-bottom: 1px solid #b7a075;">
    <%== prediction_for_print(locals[:prediction]) %>
  </td>
</tr>