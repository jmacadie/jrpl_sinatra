<h5>Table</h5>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Player</th>
      <th>Prediction</th>
      <th>Points</th>
    </tr>
  </thead>
  <tbody>
    <% @predictions.each do |prediction| %>
    <tr>
      <td><%== prediction[:user] %></td>
      <td><%== prediction_for_print(prediction) %></td>
      <td><%== prediction_points_for_print(prediction) %></td>
    </tr>
    <% end %>
  </tbody>
</table>