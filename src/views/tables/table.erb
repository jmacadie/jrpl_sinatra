<table class="table table-striped">
  <thead>
    <th>Rank</th>
    <th>User</th>
    <th>Result points</th>
    <th>Score points</th>
    <th>Total points</th>
  </thead>
  <tbody>
    <% locals[:table].each do |user_data|%>
      <tr>
        <td><%== user_data[:rank] %></td>
        <td><%== user_data[:user_name] %></td>
        <td><%== points_for_print(user_data[:result_points]) %></td>
        <td><%== points_for_print(user_data[:score_points]) %></td>
        <td><%== points_for_print(user_data[:total_points]) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

